#include <stdio.h>
#define NUMVAL 20
/*acquisisce 20 interi, un valore soglia e poi calcola e visualizza il numero di valori acquisiiti strettamente maggiori alla soglia*/
int main(int argc, char*argv[]) {

	int numeri[NUMVAL]; /* int numeri[5]; */
	int i = 0, soglia, cont = 0;
	
	while(i < NUMVAL){
	scanf("%d", &numeri[i]);
	i++;
	}
	/* i vale 20 */
	scanf("%d", &soglia);
	
	i--;
	while(i >= 0){
	if (numeri[i] > soglia)
		cont++;
	i--;
	}
	
	/* (soluzione convenzionalmente miglore)
	i = 0;
	while(i < 0){
	if (numeri[i] > soglia)
		cont++;
	i++;
	}
	*/
	
	printf("%d\n", nchamp);
	
	return 0;
	
}
