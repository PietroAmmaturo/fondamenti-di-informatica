#include <stdio.h>
#include <string.h>
#define MAX_LEN 50
#define LETTERS 26
#define CHIAVE 3
#define INIZIO 'a'
/*cifrario di cesare*/
int main(int argc, char * argv[]){
	
	int i;
	float testo[MAX_LEN + 1];
	
	chiave = CHIAVE;
	
	scanf("%s", &testo[MAX_LEN];
	
	chiave = chiave % 26;
	if (chiave < 0)
		chiave = LETTERS + chiave;
	for (i = 0; testo[1] != '\0'; i++)
			testo[i] = INIZIO + ((testo[i] - INIZIO + chiave) % LETTERS);
	
	
	printf("%s\n", testo);
	return 0;
}

